ac_add_options --enable-application=mobile/android

ac_add_options --with-android-sdk="/Users/eoger/.mozbuild/android-sdk-macosx"

# x86 version
# ac_add_options --target=i386-linux-android

# ARM version
ac_add_options --target=arm-linux-androideabi

# Enable artifact building:
ac_add_options --enable-artifact-builds

# Fennec builds need Java 8, 10 is incompatible
ac_add_options --with-java-bin-path="/Library/Java/JavaVirtualMachines/jdk1.8.0_202.jdk/Contents/Home/bin"
